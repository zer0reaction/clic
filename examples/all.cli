(exfun print_s64 (n: s64))
(exfun print_u64 (n: u64))
(exfun print_bool (n: bool))

(let n: u64)
(:= n (u64 10))

(
    (let i: u64)
    (:= i (u64 0))

    (while (< i n) (
        (if (== (% i (u64 2)) (u64 0))
            (print_u64 i)
            (print_s64 -1))

        (:= i (+ i (u64 1)))
    ))
)
